                     postgres    false            √µ            1255    24871    actualizar_total_neto_semana()    FUNCTION     ¬Å  CREATE FUNCTION public.actualizar_total_neto_semana() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
DECLARE
  semana_id INTEGER;
BEGIN
  -- Detecta si fue INSERT o UPDATE
  IF (TG_OP = 'INSERT' OR TG_OP = 'UPDATE') THEN
    semana_id := NEW.id_semana;
  ELSE
    semana_id := OLD.id_semana;
  END IF;

  -- Calcula la suma de total_neto para la semana
  INSERT INTO resumen_nomina(id_semana, total_semana)
  VALUES (
    semana_id,
    (SELECT COALESCE(SUM(total_neto),0) FROM nomina_empleados_semanal WHERE id_semana = semana_id)
  )
  ON CONFLICT (id_semana)
  DO UPDATE SET total_semana = EXCLUDED.total_semana;

  RETURN NULL;
END;
$$;
 5   DROP FUNCTION public.actualizar_total_neto_semana();
       public               postgres    false            √¥            1255    24815 7   obtener_nomina_por_cuadrilla_y_semana(integer, integer)    FUNCTION     √Ø  CREATE FUNCTION public.obtener_nomina_por_cuadrilla_y_semana(semanaid integer, cuadrillaid integer) RETURNS TABLE(id_empleado integer, nombre text, codigo text, dia_1 integer, dia_2 integer, dia_3 integer, dia_4 integer, dia_5 integer, dia_6 integer, dia_7 integer, total integer, debe integer, subtotal integer, comedor boolean)
    LANGUAGE plpgsql
    AS $$
BEGIN
  RETURN QUERY
  SELECT 
    e.id_empleado,
    e.nombre,
    e.codigo,
    n.dia_1,
    n.dia_2,
    n.dia_3,
    n.dia_4,
    n.dia_5,
    n.dia_6,
    n.dia_7,
    n.total,
    n.debe,
    n.subtotal,
    n.comedor
  FROM nomina_empleados_semanal n
  INNER JOIN empleados e ON e.id_empleado = n.id_empleado
  WHERE n.id_semana = semanaId AND n.id_cuadrilla = cuadrillaId;
END;
$$;
